
Table "artifacts" {
  "id" integer [pk, not null]
  "name" text [unique, not null]
  "description" text
  "photos" "text[]"
  "date" text
  "source" text
  "origin" text
  "images" "text[]"
  "xata_id" text
}

Table "documents" {
  "id" integer [pk, not null]
  "file" "text[]"
  "content" text
  "title" text [not null]
  "date" timestamp
  "author_id" integer
  "organization_id" integer
  "url" text
}

Table "event_subject_matter_experts" {
  "id" integer [pk, not null]
  "event_id" integer
  "subject_matter_expert_id" integer
}

Table "event_topic_subject_matter_experts" {
  "id" integer [pk, not null]
  "event_id" integer
  "topic_id" integer
  "subject_matter_expert_id" integer
}

Table "events" {
  "id" integer [pk, not null]
  "name" text [not null]
  "description" text
  "location" text
  "latitude" doubleprecision
  "longitude" doubleprecision
  "date" timestamp
  "photos" "text[]"
  "metadata" json [default: `'{}'::json`]
  "title" text [unique, not null]
  "summary" text
}

Table "mindmaps" {
  "id" integer [pk, not null]
  "json" json [default: `'{}'::json`]
  "user_id" integer
  "file" text
}

Table "organization_members" {
  "id" integer [pk, not null]
  "member_id" integer
  "organization_id" integer
}

Table "organizations" {
  "id" integer [pk, not null]
  "name" text [not null]
  "specialization" text
  "description" text
  "photo" text
  "image" text
  "title" text [unique, not null]
}

Table "personnel" {
  "id" integer [pk, not null]
  "bio" text
  "role" text
  "facebook" text
  "twitter" text
  "website" text
  "instagram" text
  "photo" "text[]"
  "rank" integer
  "credibility" integer
  "popularity" integer
  "name" text [unique, not null]
  "authority" integer
}

Table "sightings" {
  "id" integer [pk, not null]
  "date" timestamp
  "description" text
  "media_link" text
  "city" text
  "state" text
  "country" text
  "shape" text
  "duration_seconds" text
  "duration_hours_min" text
  "comments" text
  "date_posted" timestamp
  "latitude" doubleprecision
  "longitude" doubleprecision
}

Table "tags" {
  "id" integer [pk, not null]
}

Table "testimonies" {
  "id" integer [pk, not null]
