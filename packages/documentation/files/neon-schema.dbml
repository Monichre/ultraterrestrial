  "synopsis" text
  "diagrams" "text[]"

  Indexes {
    (user_id, name) [unique, name: "user_theories_user_id_name_key"]
  }
}

Table "users" {
  "id" integer [pk, not null]
  "email" text [unique, not null]
  "name" text [not null]
  "photo" text
  "profile_image_url" text
  "external_id" text
}

